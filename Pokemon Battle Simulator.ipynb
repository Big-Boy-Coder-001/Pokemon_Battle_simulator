    "    spa=(data.loc[data['Name']==a]['Sp. Atk'])\n",
    "    spd=(data.loc[data['Name']==a]['Sp. Def'])\n",
    "    spe=(data.loc[data['Name']==a]['Speed'])\n",
    "    spe2=(data.loc[data['Name']==b]['Speed'])\n",
    "    p1=[type1, type2, hp, atk, defe, spa, spd, spe, name]\n",
    "    \n",
    "    name=(data.loc[data['Name']==b]['Name'])\n",
    "    type1=(data.loc[data['Name']==b]['Type 1'])\n",
    "    type2=(data.loc[data['Name']==b]['Type 2'])\n",
